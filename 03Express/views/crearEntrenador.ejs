<%- include("template/cabecera", {tituloWeb: 'Nuevo Entrenador' }) %>
    <div class="container">
        <h1>Crear nuevo Entrenador</h1>
        <form action="/entrenador" class="my-2" method="POST">
            <input type="text" placeholder="Nombre Entrenador" class="form-control my-2" name="nombre">
            <input type="text" placeholder="Genero" class="form-control my-2" name="genero">
            <input type="text" placeholder="Región" class="form-control my-2" name="region">
            <input type="text" placeholder="Gimnasio" class="form-control my-2" name="gimnasio">
            <input type="text" placeholder="Descripción" class="form-control my-2" name="descripcion">
            <button type="submit" class="btn btn-success btn-block">
                Agregar
            </button>
        </form>
        <a class="btn btn-dark btn-block my-4" href="/entrenador">Volver</a>
    </div>
    <%- include("template/footer") %>